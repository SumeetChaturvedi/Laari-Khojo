{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "installCommand": "npm install",
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@2.6.9"
    }
  },
  "routes": [
    { "src": "/api/auth/(.*)", "dest": "api/auth/$1.js" },
    { "src": "/api/(.*)", "dest": "api/$1.js" }
  ]
}